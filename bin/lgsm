#!/usr/bin/env python

import sys, os
from pprint import pprint
apath = os.path.abspath(os.path.realpath(os.path.dirname(__file__) + '/..'))
sys.path.insert(0, apath)
print apath

import lgsm

conf = lgsm.GameConfig('insserver')
print "config"
pprint(conf.config)
print "dump config"
print conf.dump_config()
print "done"
